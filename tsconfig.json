{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
 "moduleResolution": "node",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "lib": ["DOM", "ESNext"],
    "sourceMap": true,
    "jsx": "react-jsx",
    "declaration": true,
  "emitDeclarationOnly": true,
  "isolatedModules": true,
  "allowImportingTsExtensions": true

  },
  "include": ["src/**/*", "type.ts" ],
  "exclude": ["node_modules"]
}
