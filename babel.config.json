{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript"
  ],
  "plugins": [
    ["@babel/plugin-transform-react-jsx", {
      "pragma": "ReactSphere.createElement",
      "pragmaFrag": "ReactSphere.Fragment"
    }],
    "@babel/plugin-transform-runtime",
    "babel-plugin-add-import-extension"
  ]
}
