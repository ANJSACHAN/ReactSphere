{
  "presets": [
    ["@babel/preset-env", {
      "modules": false,
      "targets": {
        "browsers": "defaults"
      }
    }],
    "@babel/preset-typescript",
  
  ],
  "plugins": [
    ["babel-plugin-add-import-extension", {
      "extension": "js",
      "replace": true
    }],
      "@babel/plugin-transform-react-jsx"

  ]
}
